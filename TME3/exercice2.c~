#include<stdlib.h>
#include <stdio.h>
#define TAILLE 256
int main(){
  static const char filename[] = "data/email-Eu-core.txt";
  FILE* file = fopen(filename,"r");

  int max = 0;
  int nbLiens = 0;
  char buf[TAILLE];
  while(fgets(buf,TAILLE,file)!=NULL){
   if(buf[0]!='#'){
    int a,b;
    sscanf(buf,"%d %d\n",&a,&b);
    if(a>max) max = a;
    if(b>max) max = b;
    nbLiens++;
    }
  }

  printf("the number of node is %d\n",max);
  printf("the number of edges is %d\n",nbLiens);
  return 0;


}
